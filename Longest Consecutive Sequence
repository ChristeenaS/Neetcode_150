Brute Force

class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        max_length = 0

        for num in nums:
            current_num = num
            streak = 1

            while current_num +1 in nums:
                current_num +=1
                streak +=1
            
            max_length = max(max_length,streak)
        
        return max_length
